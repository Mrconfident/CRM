A;;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,cAAA,GAAA,sBAAA;;AACA,cAAA,OAAA,GAAA;AACA,YAAA,aAAA,EAAA,SAAA,CAAA,MAAA;AADA,WAAA;AAGA,UAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,aAAA;AACA,UAAA,KAAA,CAAA,GAAA,CAAA,IAAA,EAAA;AAAA,YAAA,OAAA,EAAA;AAAA,WAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,gBAAA,GAAA,CAAA,IAAA,KAAA,mBAAA,EAAA;AACA,cAAA,KAAA,CAAA,mBAAA,CAAA;AACA,aAFA,MAEA;AACA,cAAA,MAAA,CAAA,IAAA,CAAA,oBAAA;AACA;AACA,WAPA;AAQA;AACA,OArBA,EAqBA,KArBA,CAqBA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAvBA;AAwBA;AA3BA;AANA,CAAA","sourcesContent":["<template>\n<div class=\"LoginBusiness\">\n  <h2>Organisation Login</h2>\n  <div class=\"container\">\n    <input type=\"text\" placeholder=\"Enter Username\" name=\"uname\" v-model=\"username\" required>\n    <br/>\n    <input type=\"password\" placeholder=\"Enter Password\" name=\"psw\" v-model=\"password\" required>\n    <br>\n    <button type=\"submit\" @click=\"authenticate\">Login</button>\n  </div>\n\n  <div class=\"container\">\n    <span class=\"psw\">Login Agent...? <router-link to=\"/loginagent\"> Login </router-link></span>\n  </div>\n</div>\n</template>\n<script>\nimport axios from 'axios'\nimport router from '../router/index'\nimport { setCookie, getCookie } from '../cookies'\nimport api from '../config/api'\nexport default {\n  name: 'LoginBusiness',\n  data: () => ({\n    username: '',\n    password: ''\n  }),\n  methods: ({\n    authenticate () {\n      const url = api.BASE_URL + 'auth/authenticate'\n      axios.post(url, { username: this.username, password: this.password, client: 'crm' }).then((response) => {\n        console.log(response)\n        if (response.data.success !== true) {\n          alert(response.data.error)\n        } else {\n          setCookie('b_id', response.data.data.jwt, 10)\n          console.log(response.data + '************************')\n          const url = api.BASE_URL_LOCAL + 'crmorg/validateAdmin'\n          const headers = {\n            Authorization: getCookie('b_id')\n          }\n          console.log(headers.Authorization)\n          axios.get(url, { headers: headers }).then((res) => {\n            console.log(res.data)\n            if (res.data === 'No user Available') {\n              alert('User is not there')\n            } else {\n              router.push('/businessdashboard')\n            }\n          })\n        }\n      }).catch(e => {\n        console.log(e)\n      })\n    }\n  })\n}\n</script>\n<style scoped>\ninput[type=text], input[type=password] {\n  width: 25%;\n  padding: 12px 20px;\n  margin: 8px 0;\n  display: inline-block;\n  border-bottom: 1px solid #ccc;\n  border-top: none;\n  border-left: none;\n  border-right: none;\n  box-sizing: border-box;\n}\nh2{\n  font-size: 30px;\n}\nbutton {\n  margin-top:17px;\n  background-color: #123813;\n  color: white;\n  padding: 14px 20px;\n  border: none;\n  cursor: pointer;\n  width: 25%;\n  border-radius: 10px;\n}\na, a:visited {\n    color: black;\n    text-decoration: none;\n}\n\nbutton:hover {\n  opacity: 0.8;\n}\n\n.cancelbtn {\n  width: auto;\n  padding: 10px 18px;\n  background-color: #f44336;\n}\n.container {\n  padding: 8px;\n}\n\nspan.psw {\n  float: center;\n  padding-top: 16px;\n  align-content: center;\n}\n.LoginBusiness{\n  margin-top:7% ;\n}\n</style>\n"],"sourceRoot":"src/views"}]}